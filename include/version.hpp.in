#ifndef TMR_LISTENER_VERSION_HPP_
#define TMR_LISTENER_VERSION_HPP_

#define TMR_LISTENER_VERSION_COMBINED(major, minor, patch) ((major)*1000000 + (minor)*10000 + (patch))

#define TMR_LISTENER_VERSION_STRING "@tmr_listener_VERSION@"
#define TMR_LISTENER_VERSION_MAJOR (@tmr_listener_VERSION_MAJOR@ + 0)
#define TMR_LISTENER_VERSION_MINOR (@tmr_listener_VERSION_MINOR@ + 0)
#define TMR_LISTENER_VERSION_PATCH (@tmr_listener_VERSION_PATCH@ + 0)
#define TMR_LISTENER_VERSION TMR_LISTENER_VERSION_COMBINED(@tmr_listener_VERSION_MAJOR@ + 0, @tmr_listener_VERSION_MINOR@ + 0, @tmr_listener_VERSION_PATCH@ + 0)

#define TMR_LISTENER_SUPPORT_TMFLOW_VERSION_STRING "@tmr_listener_TMFLOW_SUPPORT_VERSION@"
#define TMR_LISTENER_SUPPORT_TMFLOW_VERSION_MAJOR (@tmr_listener_TMFLOW_SUPPORT_VERSION_MAJOR@ + 0)
#define TMR_LISTENER_SUPPORT_TMFLOW_VERSION_MINOR (@tmr_listener_TMFLOW_SUPPORT_VERSION_MINOR@ + 0)
#define TMR_LISTENER_SUPPORT_TMFLOW_VERSION TMR_LISTENER_VERSION_COMBINED(@tmr_listener_TMFLOW_SUPPORT_VERSION_MAJOR@ + 0, @tmr_listener_TMFLOW_SUPPORT_VERSION_MINOR@ + 0, 0)

#define CURRENT_TMFLOW_VERSION_GE(major, minor, patch) TMR_LISTENER_SUPPORT_TMFLOW_VERSION >= TMR_LISTENER_VERSION_COMBINED(major, minor, patch)

#endif